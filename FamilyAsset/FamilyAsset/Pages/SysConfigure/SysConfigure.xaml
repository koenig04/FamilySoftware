<UserControl x:Class="FamilyAsset.Pages.SysConfigure.SysConfigure"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:itemconfig="clr-namespace:FamilyAsset.Pages.SysConfigure.Element.ItemConfigure"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="750">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <itemconfig:ItemConfigure Grid.Row="1" Grid.Column="0" DataContext="{Binding ItemConfig}"/>
    </Grid>
</UserControl>
